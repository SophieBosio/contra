plusOne :: Integer -> Integer .
plusOne x = x + 1 .

plusOneGt :: Integer -> Boolean .
plusOneGt x =*= (plusOne x) > x .

plusOneTwiceGt :: Integer -> Boolean .
plusOneTwiceGt x =*= (plusOne (plusOne x)) > x .

plus :: Integer -> Integer -> Integer .
plus x y = x + y .

wrongPlusCommutative :: Integer -> Integer -> Boolean .
wrongPlusCommutative x y =*= (plus x y) > (plus y x) .

